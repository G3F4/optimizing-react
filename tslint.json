{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": {
      "options": ["single", "jsx-double"]
    },
    "interface-name": {
      "options": "never-prefix"
    },
    "variable-name": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "semicolon": true
  },
  "rulesDirectory": []
}
